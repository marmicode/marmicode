  "packageManager": "yarn@3.5.1",
  "scripts": {
    "postinstall": "tools/fix-yarn-pnp.sh",
    "nx": "pnpify nx",
    "nx-cloud": "pnpify nx-cloud",
    "start": "nx serve",
    "build": "nx build",
    "component-test": "nx run-many --target=component-test",
    "test": "nx run-many --target=test",
    "lint": "nx workspace-lint && nx run-many --target=lint",
    "e2e": "nx run-many --target=e2e",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "build-github-workflows": "ts-node -P tools/build-github-workflows/tsconfig.lib.json tools/build-github-workflows/src/main.ts",
    "deploy:next": "nx run marmicode:deploy:next",
    "deploy:prod": "nx run marmicode:deploy:production",
    "graphql-codegen": "graphql codegen",
    "test:watch": "nx affected:test --watch --all --parallel --maxParallel 100",
    "upload-affected-percy-snapshots": "yarn nx run percy-placeholder:upload-percy-snapshots && yarn nx affected --target=upload-percy-snapshots --base=deployed/marmicode-next",
    "workspace-generator": "nx workspace-generator",
    "_dev:ssr": "nx run marmicode:serve-ssr",
    "_serve:ssr": "node dist/marmicode/server/main.js",
    "_build:ssr": "nx build --prod && nx run marmicode:server:production",
    "_prerender": "nx run marmicode:prerender"
  },
