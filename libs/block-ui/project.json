{
  "name": "block-ui",
  "projectType": "library",
  "sourceRoot": "libs/block-ui/src",
  "prefix": "marmicode",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/block-ui/src/**/*.ts",
          "libs/block-ui/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/block-ui/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": [
        "{workspaceRoot}/coverage/libs/block-ui"
      ]
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/block-ui/.storybook",
        "browserTarget": "block-ui:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputDir": "dist/storybook/recipe-ui",
        "configDir": "libs/block-ui/.storybook",
        "browserTarget": "block-ui:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "component-test-build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "does-not-exist",
        "index": "does-not-exist",
        "main": "does-not-exist",
        "polyfills": "libs/block-ui/src/polyfills.ts",
        "tsConfig": "libs/block-ui/tsconfig.lib.json"
      }
    },
    "component-test": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "libs/block-ui/cypress.config.ts",
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "block-ui:component-test-build"
      }
    }
  },
  "generators": {},
  "tags": [
    "scope:block",
    "type:ui"
  ]
}
