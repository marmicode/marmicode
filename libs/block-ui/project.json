{
  "name": "block-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/block-ui/src",
  "prefix": "marmicode",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/block-ui/src/**/*.ts",
          "libs/block-ui/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "libs/block-ui/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/libs/block-ui"]
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/block-ui/.storybook",
        "browserTarget": "block-ui:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputDir": "dist/storybook/recipe-ui",
        "configDir": "libs/block-ui/.storybook",
        "browserTarget": "block-ui:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "component-test-build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "does-not-exist",
        "index": "does-not-exist",
        "main": "does-not-exist",
        "polyfills": "libs/block-ui/src/polyfills.ts",
        "tsConfig": "libs/block-ui/tsconfig.lib.json"
      }
    },
    "component-test": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "libs/block-ui/cypress.config.js",
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "block-ui:component-test-build",
        "browser": "chrome"
      }
    },
    "upload-percy-snapshots": {
      "command": "tools/upload-percy-snapshots.sh libs/block-ui"
    }
  },
  "generators": {},
  "tags": ["scope:block", "type:ui"]
}
