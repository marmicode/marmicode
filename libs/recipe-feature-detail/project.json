{
  "name": "recipe-feature-detail",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/recipe-feature-detail/src",
  "prefix": "mc",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/recipe-feature-detail/src/**/*.ts",
          "libs/recipe-feature-detail/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "libs/recipe-feature-detail/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/libs/recipe-feature-detail"]
    }
  },
  "generators": {},
  "tags": ["scope:recipe", "type:feature"]
}
