{
  "projectType": "library",
  "root": "libs/resource-core",
  "sourceRoot": "libs/resource-core/src",
  "prefix": "mc",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/resource-core/src/**/*.ts",
          "libs/resource-core/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/resource-core/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/resource-core"]
    }
  },
  "generators": {},
  "tags": ["scope:resource", "type:core"]
}
