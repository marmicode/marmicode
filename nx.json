{
  "npmScope": "marmicode",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tslint.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "block-api": {
      "tags": [
        "scope:block",
        "name:block-api",
        "type:api"
      ]
    },
    "block-core": {
      "tags": [
        "scope:block",
        "type:core"
      ]
    },
    "block-ui": {
      "tags": [
        "scope:block",
        "type:ui"
      ]
    },
    "block-ui-e2e": {
      "tags": [],
      "implicitDependencies": [
        "block-ui"
      ]
    },
    "blog-post-feature-detail": {
      "tags": [
        "scope:blog-post",
        "type:feature"
      ]
    },
    "blog-post-ui": {
      "tags": [
        "scope:blog-post",
        "type:ui"
      ]
    },
    "blog-post-ui-e2e": {
      "tags": [],
      "implicitDependencies": [
        "blog-post-ui"
      ]
    },
    "contentful-api": {
      "tags": [
        "scope:contentful",
        "type:api",
        "name:contentful-api"
      ]
    },
    "learning-map-feature-tree": {
      "tags": [
        "scope:learning-map",
        "type:feature"
      ]
    },
    "marmicode": {
      "tags": [
        "scope:app"
      ]
    },
    "marmicode-e2e": {
      "tags": [
        "scope:e2e"
      ],
      "implicitDependencies": [
        "marmicode"
      ]
    },
    "recipe-feature-detail": {
      "tags": [
        "scope:recipe",
        "type:feature"
      ]
    },
    "resource-api": {
      "tags": [
        "scope:resource",
        "type:api",
        "name:resource-api"
      ]
    },
    "resource-core": {
      "tags": [
        "scope:resource",
        "type:core"
      ]
    },
    "resource-feature-search": {
      "tags": [
        "scope:resource",
        "type:feature"
      ]
    },
    "resource-ui": {
      "tags": [
        "scope:resource",
        "type:ui"
      ]
    },
    "services-feature-presentation": {
      "tags": [
        "scope:services",
        "type:feature"
      ]
    },
    "shared-router-helpers": {
      "tags": [
        "scope:shared",
        "type:utils"
      ]
    },
    "shared-ui": {
      "tags": [
        "scope:shared",
        "type:ui"
      ]
    },
    "shared-utils": {
      "tags": [
        "scope:shared",
        "type:utils"
      ]
    },
    "testing": {
      "tags": [
        "scope:shared",
        "type:utils"
      ]
    },
    "tools-build-github-workflows": {
      "tags": []
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "marmicode",
  "generators": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  }
}
