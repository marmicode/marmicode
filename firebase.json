{
  "hosting": [
    {
      "target": "marmicode",
      "public": "dist/apps/marmicode/static",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "trailingSlash": false
    }
  ]
}
